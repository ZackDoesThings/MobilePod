!
!
!
enable
terminal length 0
conf t
hostname mIsIL022a02a01
enable secret cisco
username cisco password cisco
line con 0
logging synchronous
login local
no ip domain lookup
!
line vty 0 4
logging synchronous
login local
no ip domain lookup
vtp mode transparent
!
!
!
*********** VLANS ****************
!
!

vlan 90
name Routing
exit

vlan 1220
name Users
exit
!

vlan 2220
name WLAN_users
exit
!

vlan 3220
name servers
exit
!

vlan 914
name P2P
exit
!
!
!
ip routing
!
!
!
interface loop0
description loopback for L3 switch
ip address 10.2.22.194 255.255.255.255
shut
!
no shut
!
exit
!
!
!
interface vlan 90
ip address 10.2.22.197 255.255.255.248
description Port aggreagation interface to mLsa02a02
shut
!
no shut!
!
exit!
!
!
!
interface vlan 1220
ip address 10.2.22.210 255.255.255.248
description Management SVI default gateway for wgsa02a01
ip helper-address 10.2.22.192
standby 120 ip 10.2.22.209
standby 120 priority 110
standby 120 preempt
shut!
!
no shut!
!
exit!
!
!
!
interface vlan 2220
ip address 10.2.22.225 255.255.255.224
description Management SVI default gateway for wgsa02a02
ip helper-address 10.2.22.192
standby 220 ip 10.2.26.227
standby 220 priority 110
standby 220 preempt
shut!
!
no shut!
!
exit!
!
!
!
interface vlan 3220
ip address 10.2.22.218 255.255.255.248
description Management SVI default gateway for Server
standby 32 ip 10.2.26.217
standby 32 priority 110
standby 32 preempt
shut!
!
no shut!
!
exit!

int vlan 914 
description SVI ILvlanP2P
ip address 10.2.119.114 255.255.255.252
shut
no shut
exit
!


************* Interfaces ***********


