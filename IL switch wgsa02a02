!
enable
terminal length 0
conf t
hostname wgsIL022a02a02
enable secret cisco
username cisco password cisco
line con 0
logging synchronous
login local
no ip domain lookup
line vty 0 4
logging synchronous
login local
no ip domain lookup
!
!
vlan 2220
name MGMT
exit!
!
!
!
interface vlan 2220
ip address 10.2.22.230 255.255.255.224
description Management SVI for vlan 2220
shut!
!
no shut!
!
exit!
!
!
!
interface f0/1
description trunk interface from wgsa02a02 to mIsIL022a02a01 switch
switchport trunk encapsulation dot1q
switchport mode trunk
switchport trunk allowed vlan 1220,2220
shut!
!
no shut!
!
exit!
!
!
!
interface f0/2
description trunk interface from wgsa02a02 to mIsIL022a02a02 switch
switchport trunk encapsulation dot1q
switchport mode trunk
switchport trunk allowed vlan 1220,2220
shut!
!
no shut!
!
exit!
